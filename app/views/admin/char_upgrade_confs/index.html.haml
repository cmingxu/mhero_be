#json_editor{:style => "width: 400px; height: 400px"}

:javascript
  var container = document.getElementById("json_editor");
  var editor = new jsoneditor.JSONEditor(container);
  var json = {
              "Array": [1, 2, 3],
              "Boolean": true,
              "Null": null,
              "Number": 123,
              "Object": {"a": "b", "c": "d"},
              "String": "Hello World"
          };
  editor.set(json);
    
  

