= link_to glyphicon_btn("plus", "New"), new_admin_uniq_user_path, :class => "pull-right btn btn-default"
%table.table
  %thead
    %th ID
    %th Email
    %th

  %tbody
    - @uniq_users.each do |uu|
      %tr
        %td= uu._id
        %td= uu.email
        %td= uu.last_login_at


= paginate @uniq_users
